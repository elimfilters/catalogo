{
  "version": "7.0.0",
  "description": "ELIMFILTERS LT MASTER RULES",

  "rules": {
    "security": {
      "block_on_rule_violation": true,
      "strict_mode": true,
      "integrity_required": true
    },

    "scraping": {
      "Fleetguard": {
        "allowed_for": ["HD", "LD"],
        "columns_allowed": [
          "Contenido del Kit",
          "Filtro Principal (Ref)",
          "Tecnología"
        ]
      },

      "FRAM": {
        "allowed_for": ["HD", "LD"],
        "columns_allowed": [
          "Contenido del Kit",
          "Filtro Principal (Ref)",
          "Tecnología"
        ]
      },

      "Donaldson": {
        "allowed_for": ["HD", "LD"],
        "columns_allowed": [
          "Contenido del Kit",
          "Filtro Principal (Ref)",
          "Tecnología"
        ]
      }
    },

    "vin_engine": {
      "validation_regex": "^[A-HJ-NPR-Z0-9]{17}$"
    },

    "technology_assignment": {
      "allowed_values": [
        "ELIMTEK™ MultiCore",
        "ELIMTEK™ Blend",
        "MACROCORE™ Ultra",
        "MACROCORE™ Plus",
        "HYDROFLOW™ 5000",
        "AQUACORE™ Pro",
        "NANOMAX™ Media",
        "ELIMTEK™ Standard"
      ]
    }
  }
}
