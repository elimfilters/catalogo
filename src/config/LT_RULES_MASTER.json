{
  "version": "7.0.0",
  "description": "ELIMFILTERS LT MASTER RULESET – INMUTABLE CONTRACT",
  "rules": {
    "security": {
      "block_on_rule_violation": true,
      "strict_mode": true,
      "integrity_required": true
    },

    "scraping": {
      "Fleetguard": {
        "allowed_for": "HD",
        "columns_allowed": [
          "Contenido del Kit",
          "Filtro Principal (Ref)",
          "Tecnología"
        ]
      },
      "FRAM": {
        "allowed_for": "LD",
        "columns_allowed": [
          "Contenido del Kit",
          "Filtro Principal (Ref)",
          "Tecnología"
        ]
      },
      "Donaldson": {
        "allowed_for": "HD",
        "columns_allowed": [
          "Contenido del Kit",
          "Filtro Principal (Ref)",
          "Tecnología"
        ]
      }
    },

    "sku_generation": {
      "HD": {},
      "LD": {},
      "LT_INTERNAL": {},
      "notes": "SKU prefixes are NOT generated here. Controlled ONLY by /src/config/prefixes.js + /src/sku/generator.js"
    },

    "technology_assignment": {
      "allowed_values": [
        "ELIMTEK™ Standard"
      ],
      "notes": "Advanced tiers handled externally (Blend, MultiCore, MacroCore, HydroFlow, AquaCore, NanoMax, etc.)"
    },

    "vin_engine": {
      "validation_regex": "^[A-HJ-NPR-Z0-9]{17}$",
      "notes": "Strict VIN validation required for automotive applications"
    },

    "compliance": {
      "prevent_prefix_generation": true,
      "blocked_actions": [
        "derive_EA1_from_brand",
        "derive_EL8_from_brand",
        "derive_EF9_from_brand",
        "derive_EC1_from_brand",
        "derive_EH6_from_brand",
        "derive_ES9_from_brand",
        "derive_EW7_from_brand",
        "derive_ET9_from_brand",
        "derive_EM9_from_brand",
        "derive_EK3_from_brand",
        "derive_EK5_from_brand"
      ],
      "notes": "OEM brand detection CANNOT generate ELIMFILTERS prefixes"
    }
  }
}